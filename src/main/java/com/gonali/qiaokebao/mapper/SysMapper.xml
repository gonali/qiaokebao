<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gonali.qiaokebao.mapper.SysMapper">

    <select id="findById" resultType="Sys">
        SELECT * FROM tbSys WHERE id = #{id}
    </select>

    <select id="findByName" resultType="Sys">
        SELECT * FROM tbSys WHERE name=#{name}
    </select>

</mapper>